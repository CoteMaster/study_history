#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int solution(vector<vector<int>> triangle) {
    int answer = 0;
    int size = triangle.size();
    vector<vector<int>> temp(size, vector<int>(size));
    
    temp[0][0] = triangle[0][0];
    
    for(int i = 0; i < size; i++) {
        for(int j = 0; j <= i; j++){
            if (j == 0) //Left
                temp[i][0] = temp[i-1][0] + triangle[i][0];
            else if (j == i) //Right
                temp
            else {
                temp[i][j] = max(temp[i-1][j-1]) + triangle[i][j];
            }
        }
    }
    return answer;
}
